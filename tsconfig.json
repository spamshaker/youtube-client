{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "preserveSymlinks": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "outDir": "generated",
    "paths": {
      "@web-modules/*": ["client/web/src/*"]
    }
  }
}
