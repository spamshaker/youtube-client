  },
  "homepage": "https://github.com/spamshaker/youtube-client#readme",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@ria-develop/eslint-config": "^0.0.10",
    "@types/express": "^4.17.13",
    "@types/gapi": "^0.0.41",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/youtube": "^0.0.44",
